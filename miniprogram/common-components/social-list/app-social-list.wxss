.social-content {
    width: 100%;
}

.social-bottom {
    position: fixed;
    bottom: 51rpx;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
    margin-left: -30rpx;
}

.call-teacher {
    width:380rpx;
    height:104rpx;
    border-radius:52px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:48rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(255,255,255,1);
}

.call-teacher-player {
    font-size: 104rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #FFFFFF;
    position: absolute;
    right: 47rpx;
    border-radius: 50%;
    overflow: hidden;
}

.no-list {
    width: 100%;
    padding: 255rpx 0 0 0;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.no-list-title {
    font-size:45rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    color:rgba(153,153,153,1);
}

.no-list-subtitle {
    font-size:45rpx;
    font-family:PingFangSC-Regular,PingFang SC;
}

.social-list-header {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.social-list-header-left {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.top-cicle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
}

.bottom-cicle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background:rgba(61,61,61,1);
    margin-top: 7rpx;
}

.have-list {
    padding-top: 39rpx;
}

.header-title {
    font-size:32rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(68,66,66,1);
    margin-left: 19rpx;
}

.social-list {
    margin-top: 17rpx;
    border-radius:14px;
    overflow: hidden;
}

.wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.block {
    width: 588rpx;
    height: 1030rpx;
    position: absolute;
    top: 21rpx;
}

.block-card {
    width: 588rpx;
    height: 890rpx;
    box-sizing: border-box;
    padding: 37rpx 50rpx;
    background:rgba(255,255,255,1);
    border-radius:6px;
    border:1px solid rgba(151,151,151,1);
}

.card-header {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    padding-bottom: 35rpx;
    box-sizing: border-box;
    border-bottom: 1rpx solid #E1E0E0;
}

.card-user-img {
    width:82rpx;
    height:82rpx;
    border-radius: 50%;
    overflow: hidden;
}

.card-header-right {
    margin-left: 18rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
}

.card-user-name {
    font-size:32rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(7,7,7,1);
}

.card-user-des {
    font-size:28rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(197,196,196,1);
}

.card-title {
    font-size:42rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(7,7,7,1);
    margin-top: 23rpx;
}

.card-sub-title {
    color:#A85347;
}

.card-content-des {
    font-size:32rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(7,7,7,1);
    margin-top: 13rpx;
}

.card-img {
    width:488rpx;
    height:276rpx;
    margin-top: 30rpx;
}

.card-bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding-top: 28rpx;
    border-top: 1rpx solid #E1E0E0;
}

.card-bottom-text {
    font-size:26rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(51,51,51,1);
}

.card-logo-text {
    color: #CE2A4E;
}

.my-mini-logo {
    width:144rpx;
    height:144rpx;
}

.posterview-canvas {
    position: absolute;
    width: 588px;
    height: 890px;
    background: white;
    visibility: hidden;
    top: -2000px;
}

.posterview-image {
    width: 588rpx;
    height: 890rpx;
}