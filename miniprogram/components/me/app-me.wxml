<view class="me-content">
    <view class="me-info">
        <view class="me-info-left">
            <view class="user-avatar-url">
                <open-data type="userAvatarUrl"></open-data>
            </view>
            <view class="user-info">
                <view class="user-name">
                    <open-data type="userNickName"></open-data>
                </view>
                <view class="grade">红松学堂</view>
            </view>
        </view>
    </view>
    <view class="me-info-buttom">
        <van-image width="605rpx" height="706rpx" lazy-load="{{true}}" src="cloud://hs-class-schedule-we-8wofx.6873-hs-class-schedule-we-8wofx-1301353511/school-card.png"></van-image>
        <view class="me-info-desc">
            <view class="me-info-cell">
                <view class="info-title">姓名:</view>
                <view class="info-des">
                    <open-data type="userNickName"></open-data>
                </view>
            </view>
            <view class="me-info-cell">
                <view class="info-title">学号:</view>
                <view class="info-des">NO.{{simpleUserModel.sno}}</view>
            </view>
            <view class="me-info-cell">
                <view class="info-title">入学日期:</view>
                <view class="info-des">{{simpleUserModel.start}}</view>
            </view>
        </view>
        <view class="hs-logo">
            <van-image width="209rpx" height="209rpx" lazy-load="{{true}}" src="cloud://hs-class-schedule-we-8wofx.6873-hs-class-schedule-we-8wofx-1301353511/hslogo.png"></van-image>
        </view>
        <view>
            <button class="share-btn" open-type='share'>
                <van-image width="43rpx" height="35rpx" lazy-load="{{true}}" src="cloud://hs-class-schedule-we-8wofx.6873-hs-class-schedule-we-8wofx-1301353511/wxlogo-withe.png"></van-image>
                <view class="tip-share-text">分享红松学堂</view>
            </button>
        </view>
    </view>
</view>