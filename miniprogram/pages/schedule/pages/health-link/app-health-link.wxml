<view class="content">
    <view class="header f-je-ac">
        <image class="header-img" lazy-load="{{true}}" src="cloud://hs-class-schedule-we-8wofx.6873-hs-class-schedule-we-8wofx-1301353511/health-header.png"></image>
        <view class="header-text">
            <view class="text-info">1 对 1</view>
            <view class="text-info">在线连麦得调理方案</view>
            <view class="header-line"></view>
        </view>
    </view>
    <view class="card-list">
        <view class="card-item" wx:for="{{healthList}}" wx:for-item="item" wx:for-index="index" wx:key="index" bindtap="onJumpLook" data-value="{{item}}">
            <app-health-card cardData="{{item}}"></app-health-card>
        </view>
    </view>
</view>