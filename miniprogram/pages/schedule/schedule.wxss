Page {
    background:rgba(255,255,255,1);
}

.schedule-content {
    padding: 0 0 250rpx 0;
}

.social-content {
    padding: 0 0 180rpx 0;
}

.me-content {
    padding: 0 0 180rpx 0;
}

.guide {
    position: fixed;
    top: 0;
    width: 100%;
    display: flex;
    justify-content: flex-end;
    z-index: 3;
}

.close-tip {
    position: absolute;
    top: 25rpx;
    right: 50rpx;
    color: white;
    width: 60rpx;
    height: 60rpx;
    display: flex;
    justify-content: flex-end;
    align-items: flex-start;
}

.guide-img {
    width: 346rpx;
    height: 145rpx;
    margin-right: 38rpx;
}

.guide-des {
    position: fixed;
    top: 0;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 145rpx;
    width: 346rpx;
    z-index: 1;
}

.guide-title {
    display: flex;
    justify-content: center;
    align-items: center;
}

.guide-text {
    font-size:32rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(255,231,48,1);
}

.point {
    width: 37rpx;
    height: 13rpx;
    margin-left: 18rpx;
}

.wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.block {
    position: absolute;
    top: 175rpx;
    width: 538rpx;
    border-radius:16px;
}

.welcome-block {
    position: absolute;
    top: 195rpx;
    background-color: #fff;
    width: 568rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.welcome-bg {
    width: 538rpx;
    height: 555rpx;
}

.welcome-content {
    position: absolute;
    top: 0;
    width: 538rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    border-radius:16px;
}

.welcome-content-title {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    margin-top: 50rpx;
}

.welcome-content-title-header {
    font-size:68rpx;
    font-family:SourceHanSansCN-Bold,SourceHanSansCN;
    font-weight:bold;
    color:rgba(179,72,70,1);
}

.welcome-content-title-des {
    font-size:40rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(20,20,20,1);
    margin-top: 20rpx;
}

.star-line-img {
    width: 432rpx;
    height: 21rpx;
    margin-top: 32rpx;
    margin-bottom: 35rpx;
}

.welcome-content-cell {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
}

.welcome-content-cell-item {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}

.welcome-content-cell-item-desc {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
    margin-left: 16rpx;
}

.welcome {
    width: 568rpx;
    height: 401rpx;
}

.welcome-title {
    margin-top: 16rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    color: rgba(236,127,42,1);
    font-weight: bold;
    font-size: 50rpx;
}

.join-btn {
    width:477rpx;
    height:112rpx;
    background:rgba(238,127,22,1);
    border-radius:114px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:40rpx;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(255,255,255,1);
    margin: 80rpx 0;
}

.item-title {
    font-size:35rpx;
    font-family:SourceHanSansCN-Bold,SourceHanSansCN;
    font-weight:bold;
    color:rgba(51,51,51,1);
}

.item-teacher {
    font-size:24rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(102,102,102,1);
}

.item-tag {
    width:76rpx;
    height:38rpx;
    background:rgba(179,72,70,1);
    border-radius:9px;
    font-size:28rpx;
    font-family:SourceHanSansCN-Bold,SourceHanSansCN;
    font-weight:bold;
    color:rgba(255,255,255,1);
    display: flex;
    justify-content: center;
    align-items: center;
}

.look-btn {
    width:170rpx;
    height:71rpx;
    background:rgba(255,255,255,1);
    border-radius:9px;
    border:2rpx solid rgba(179,72,70,1);
    font-size:36rpx;
    font-family:SourceHanSansCN-Bold,SourceHanSansCN;
    font-weight:bold;
    color:rgba(179,72,70,1);
    margin-top: 48rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.dailog-cell-item-right {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}