.banner {
    height: 220rpx;
    width: 100%;
    position: relative;
}

.banner-swiper {
    width: 100%;
    height: 220rpx;
}

.banner-img {
    width: 100%;
    height: 220rpx;
}

.banner-des {
    position: absolute;
    top: 0;
    height: 220rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
}

.banner-des-img {
    width: 521rpx;
    height: 77rpx;
    margin-top: 47rpx;
}

.banner-time {
    font-size: 36rpx;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: rgba(205, 75, 75, 1);
}

.cell {
    display: flex;
    flex-direction: column;
    padding: 100rpx 23rpx 0 23rpx;
    box-sizing: border-box;
}

.cell-header {
    position: relative;
}

.cell-header-item {
    position: absolute;
    width: 750rpx;
    left: -32rpx;
    background: rgba(247, 240, 240, 1);
    padding: 19rpx 23rpx;
    box-sizing: border-box;
}

.cell-title {
    font-size: 36rpx;
    font-family: SourceHanSansCN-Medium, SourceHanSansCN;
    font-weight: 500;
    color: rgba(119, 35, 35, 1);
}

.cell-sub-title {
    font-size:30rpx;
    font-family:SourceHanSansCN-Normal,SourceHanSansCN;
    color:rgba(119,35,35,1);
    margin-left: 30rpx;
}

.cell-item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 40rpx 0 50rpx 0;
}

.item-tag-time {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.item-middle {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-top: 19rpx;
}

.tag {
    width:99rpx;
    height:36rpx;
    background-color:rgba(248,97,97,1);
    border-radius:8px;
    font-size:30rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(255,255,255,1);
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 46rpx;
}

.item-time {
    font-size:30rpx;
    font-family:PingFangSC-Semibold,PingFang SC;
    font-weight:600;
    color:rgba(72,72,72,1);
    margin-left: 46rpx;
}

.item-title {
    font-size:36rpx;
    font-family:SourceHanSansCN-Heavy,SourceHanSansCN;
    font-weight:800;
    color:rgba(65,65,65,1);
    margin-left: 17rpx;
}

.item-teacher {
    font-size:26rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    color:rgba(120,120,120,1);
    margin-top: 24rpx;
    padding-left: 46rpx;
}

.cell-item-right {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.booking-btn {
    width:150rpx;
    height:73rpx;
    background:rgba(205,75,75,1);
    border-radius:37px;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size:36rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(255,255,255,1);
}

.booking-booked-btn {
    width:150rpx;
    height:73rpx;
    background:rgba(227,227,227,1);
    border-radius:37px;
    font-size:36rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(150,150,150,1);
    display: flex;
    justify-content: center;
    align-items: center;
}

.share-info {
    width:100%;
    height:242rpx;
    background:linear-gradient(180deg,rgba(255,255,255,0) 0%,rgba(255,255,255,0.81) 41%,rgba(255,255,255,1) 100%);
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    bottom: 0;
}

.share-btn {
    background:linear-gradient(180deg,rgba(116,206,116,1) 0%,rgba(25,173,25,1) 100%);
    display: flex;
    justify-content: center;
    align-items: center;
    width:330rpx;
    height:128rpx;
    border-radius:100px 0px 0px 100px;
    box-shadow:0px 6px 4px 0px rgba(0,0,0,0.15);
}

.share-btn-info {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 100%;
    padding: 0;
}

.share-btn-info:hover {
    background:linear-gradient(180deg,rgba(116,206,116,1) 0%,rgba(25,173,25,1) 100%);
    border-radius:100px 0px 0px 100px;
}

.join-btn {
    display: flex;
    justify-content: center;
    align-items: center;
    width:330rpx;
    height:128rpx;
    background:linear-gradient(180deg,rgba(232,102,102,1) 0%,rgba(205,75,75,1) 100%);
    border-radius:0px 100px 100px 0px;
    box-shadow:0px 6px 4px 0px rgba(0,0,0,0.15);
}

.wxlogo-img {
    width: 56rpx;
    height: 47rpx;
}

.share-text {
    margin-left: 27rpx;
    font-size:40rpx;
    font-family:SourceHanSansCN-Bold,SourceHanSansCN;
    font-weight:bold;
    color:rgba(255,255,255,1);
}

.wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
}

.block {
    width: 694rpx;
    height: 305rpx;
    background-color: #fff;
    position: absolute;
    bottom: 800rpx;
    border-radius: 10px;
}

.aways {
    width: 694rpx;
    height: 305rpx;
}

.guide {
    position: fixed;
    top: 0;
    width: 100%;
    display: flex;
    justify-content: flex-end;
    z-index: 1;
}

.guide-img {
    width: 346rpx;
    height: 145rpx;
    margin-right: 38rpx;
}

.guide-des {
    position: fixed;
    top: 0;
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    height: 145rpx;
    width: 346rpx;
    z-index: 1;
}

.guide-title {
    display: flex;
    justify-content: center;
    align-items: center;
}

.point {
    width: 37rpx;
    height: 13rpx;
    margin-left: 18rpx;
}

.guide-text {
    font-size:32rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(255,231,48,1);
}

.official-account-info {
    position: fixed;
    bottom: 40rpx;
    width: 100%;
    padding: 0 23rpx;
    box-sizing: border-box;
    border-radius: 8px;
}

.onCloseOfficialAccount{
    position: absolute;
    top: 10rpx;
    right: 30rpx;
    z-index: 1;
    color: grey;
}

.tip-content {
    padding: 44rpx 54rpx;
}

.tip-item {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-bottom: 20rpx;
}

.tip-item-content {
    font-size: 36rpx;
    font-family: SourceHanSansCN-Normal, SourceHanSansCN;
    color: rgba(90, 90, 90, 1);
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
}

.tip-qrcode-contnet {
    margin-top: 50rpx;
    border-radius: 26px;
    border: 1px solid rgba(208, 208, 208, 1);
    padding: 40rpx;
    box-sizing: border-box;
}

.tip-qrcode-title {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 32rpx;
    font-family: SourceHanSansCN-Normal, SourceHanSansCN;
    color: rgba(58, 58, 58, 1);
    margin-bottom: 20rpx;
}

.qrcode-sub-content {
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.qrcode-contnet {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.qrcode {
    width: 151rpx;
    height: 151rpx;
}

.qrcode-desc {
    font-size: 32rpx;
    font-family: SourceHanSansCN-Normal, SourceHanSansCN;
    color: rgba(58, 58, 58, 1);
    margin-top: 27rpx;
}