<view>
    <view class="schedule-content" hidden="{{active !== 0}}">
        <app-schedule scrollTop="{{scrollTop}}" showShareBtn="{{showShareBtn}}"
                bind:toastEvent="onToastEvent"
                bind:dialogEvent="onDialogEvent">
        </app-schedule>
    </view>
<view class="me-content" hidden="{{active !== 1}}">
<app-me></app-me>
</view>
    <view>
        <mp-tabbar style="position:fixed;bottom:0;width:100%;left:0;right:0;" list="{{list}}" bindchange="tabChange"></mp-tabbar>
    </view>

    <van-toast id="van-toast"/>

    <van-dialog id="van-dialog"/>

    <view class="guide" wx:if="{{guide}}">
        <image class="guide-img" src="../../images/guide.png">
            <view class="guide-des">
                <view class="guide-title">
                    <view class="guide-text">点击</view>
                    <view>
                        <image class="point" src="../../images/point.png"></image>
                    </view>
                </view>
                <view class="guide-text">“添加到我的小程序”</view>
            </view>
        </image>
    </view>
</view>