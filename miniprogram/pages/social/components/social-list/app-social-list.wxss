.social-content {
    width: 100%;
}

.no-list {
    width: 100%;
    padding: 255rpx 0 0 0;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}

.no-list-title {
    font-size:45rpx;
    font-family:PingFangSC-Regular,PingFang SC;
    color:rgba(153,153,153,1);
}

.no-list-subtitle {
    font-size:45rpx;
    font-family:PingFangSC-Regular,PingFang SC;
}

.social-list-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.header-left {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.header-right {
    display: flex;
    justify-content: center;
    align-items: center;
    width:177rpx;
    height:52rpx;
    background:rgba(168,83,71,1);
    border-radius:14rpx;
    font-size:32rpx;
    font-family:PingFangSC-Semibold,PingFang SC;
    font-weight:600;
    color:rgba(255,255,255,1);
    padding: 0;
    margin: 0;
}

.social-list-header-left {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
}

.top-cicle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
}

.bottom-cicle {
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background:rgba(61,61,61,1);
    margin-top: 7rpx;
}

.have-list {
    padding-top: 13rpx;
}

.header-title {
    font-size:32rpx;
    font-family:SourceHanSansCN-Medium,SourceHanSansCN;
    font-weight:500;
    color:rgba(68,66,66,1);
    margin-left: 19rpx;
}

.social-list {
    margin-top: 17rpx;
    border-radius:14px;
    overflow: hidden;
    margin-bottom: 200rpx;
}